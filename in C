#include<stdio.h>
#include<string.h>
#include<math.h>
#include<stdlib.h>
int main(){
int i=4;
double d=4.0;
char s[]="HackerRank";
int i1;
scanf("%d\n",&i1);
double d1;
scanf("%lf\n",&d1);
char s1[1000];
scanf("%[^\n]",s1);
printf("%d\n",i+i1);
printf("%.1lf\n",d+d1);
printf("%s",s);
printf("%s\t",s1);
return 0;
}
